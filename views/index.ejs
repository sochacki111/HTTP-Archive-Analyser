<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link href="/css/styles.css" rel="stylesheet" />
  </head>
  <body>
    <h1 class="text-center">HAR Analyzer</h1>
    <div class="text-center">
      <label for="name">URL</label>
      <input
        id="urlInput"
        type="text"
        name="url"
        placeholder="https://www.google.com"
        value="https://sleepy-mesa-09275.herokuapp.com/"
        size="100%"
        required
      />
      <button id="submitButton">Submit</button>
      <div id="logger"></div>
    </div>
    <h2 id="mediaStructureHeader"></h2>
    <div id="tree"></div>
    <h2 id="externalResourceHeader"></h2>
    <table id="externalResourceRequests" class="table table-striped"></table>
    <h2 id="cookiesHeader"></h2>
    <table id="cookies" class="table table-striped"></table>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-treeview/1.2.0/bootstrap-treeview.min.js"></script>
    <script src="/js/fetchAnalyzedHar.js"></script>
  </body>
</html>
